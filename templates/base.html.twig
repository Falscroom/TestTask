<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
            <link type="text/css" rel="stylesheet" href="{{ asset('/materialize/materialize.css') }}">
        {% endblock %}
    </head>
    <body>
        {% block logo %}
            <div class="logo">
                <img src="{{ asset('/images/logo.svg') }}">
            </div>
        {% endblock %}
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('/jQuery/jQuery.js') }}"></script>
            <script type="text/javascript" src="{{ asset('/materialize/materialize.js') }}"></script>
        {% endblock %}
    </body>
</html>
